{
  "name": "mlvc-grammar",
  "displayName": "MLVC Grammar",
  "description": "Syntax hilighting for MLVC code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mlvc",
      "aliases": ["mlvc", "mlvc"],
      "extensions": [".mlvc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mlvc",
      "scopeName": "source.mlvc",
      "path": "./syntaxes/mlvc.tmLanguage.json"
    }]
  }
}
